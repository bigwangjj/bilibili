	dnf.initJobNews={
		initJob:function(n){
			var html="",jobData=eval(raiders_guanwang_static_data2);
			for(var i=0;i<jobData[2][n].length;i++){
				var date=jobData[2][n][i].createtime;
				date=date.replace(/\-/g, "/");
				var sDate=new Date(date);
				html +='<li><span>'+sDate.getFullYear()+"/"+(sDate.getMonth()+1)+"/"+sDate.getDate()+'</span><a href="'+jobData[2][n][i].surl+'" target="_blank">'+jobData[2][n][i].title+'</a></li>';
			}
			return html
		},
		initNewsMedia:function(){
			/*var html="",jobData=eval(raiders_guanwang_static_data2);
			for(var x in jobData[1]){
				var date=jobData[1][x]['createtime'];
