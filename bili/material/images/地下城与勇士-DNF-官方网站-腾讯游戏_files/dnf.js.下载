	};
	dnf.scroller={
		intval: function(v) {
			v = parseInt(v);
			return isNaN(v) ? 0 : v
		},
		getPos: function(e) {
			var l = 0;
			var t = 0;
			var w = dnf.scroller.intval(e.style.width);
			var h = dnf.scroller.intval(e.style.height);
			var wb = e.offsetWidth;
			var hb = e.offsetHeight;
			while (e.offsetParent) {
				l += e.offsetLeft + (e.currentStyle ? dnf.scroller.intval(e.currentStyle.borderLeftWidth) : 0);
				t += e.offsetTop + (e.currentStyle ? dnf.scroller.intval(e.currentStyle.borderTopWidth) : 0);
				e = e.offsetParent
			}
			l += e.offsetLeft + (e.currentStyle ? dnf.scroller.intval(e.currentStyle.borderLeftWidth) : 0);
			t += e.offsetTop + (e.currentStyle ? dnf.scroller.intval(e.currentStyle.borderTopWidth) : 0);
			return {
				x: l,
				y: t,
				w: w,
				h: h,
				wb: wb,
				hb: hb
			}
		},
		getScroll: function() {
			var t, l, w, h;
			if (document.documentElement && document.documentElement.scrollTop) {
				t = document.documentElement.scrollTop;
				l = document.documentElement.scrollLeft;
				w = document.documentElement.scrollWidth;
				h = document.documentElement.scrollHeight
			} else if (document.body) {
				t = document.body.scrollTop;
				l = document.body.scrollLeft;
				w = document.body.scrollWidth;
				h = document.body.scrollHeight
			}
			return {
				t: t,
				l: l,
				w: w,
				h: h
			}
		},
		clearScroll:function(){
			window.clearInterval(this.timer);
			this.timer = null
		},
		init: function(el, duration) {
			if (typeof el != 'object') {
				el = document.getElementById(el)
			}
			if (!el) return;
			var z = this;
			z.el = el;
			z.p = dnf.scroller.getPos(el);
			z.s = dnf.scroller.getScroll();
			z.clear = function() {
				window.clearInterval(z.timer);
				z.timer = null
			};
			z.t = (new Date).getTime();
			z.step = function() {
				var t = (new Date).getTime();
				var p = (t - z.t) / duration;
				if (t >= duration + z.t) {
					z.clear();
					window.setTimeout(function() {
						z.scroll(z.p.y, z.p.x)
					},
					13)
				} else {
					st = (( - Math.cos(p * Math.PI) / 2) + 0.5) * (z.p.y - z.s.t) + z.s.t;
					sl = (( - Math.cos(p * Math.PI) / 2) + 0.5) * (z.p.x - z.s.l) + z.s.l;
					z.scroll(st, sl)
				}
			};
			z.scroll = function(t, l) {
				window.scrollTo(l, t)
			};
			z.timer = window.setInterval(function() {
				z.step()
			},
			13)
		}
	};
	/*dnf.scrollElent=function(){
		var elent=["week-act","video","brand","raiders","chapter","event"],elentLinks=dnf.$$("nav-fixed").getElementsByTagName("a");
		//dnf.scroller.init('week-act',800);
		for (var i = 0; i < elentLinks.length; i++) {
		(function(index, that) {
				dnf.addEventSimple(elentLinks[index], "click",
				function() {
				dnf.scroller.clearScroll();
					var t = setTimeout(function() {
						dnf.scroller.init(elent[index],800)
					},100);
				});
			})(i, this)
		}

	};*/
	dnf.tabs=function(id, cls, options){
		this.trigger = dnf.$$(id).getElementsByTagName("ul")[0].getElementsByTagName("li");
		this.count = this.trigger.length;
		this._id = id;
		this.panel = dnf.getElementsByClassName(id, cls);
		this.slider = dnf.getElementsByClassName(id, "bd")[0];
		this.t = null;
		this.index = 0;
		this.setOptions(options);
		this.onStart = this.options.onStart;
		this.event = this.options.event;
		this.timeout = this.options.timeout;
		this.onFinish = this.options.onFinish;
		this.init()
	};
	dnf.tabs.prototype={
		init: function() {
			this.trigger[0].className += " " + this.options.currCls;
			this.panel[0].className += " " + this.options.disCls;
			for (var i = 0; i < this.count; i++) { (function(index, that) {
					dnf.addEventSimple(that.trigger[index], that.options.event,
					function() {
						that.t = setTimeout(function() {
							that.switchTo(index)
						},
						that.timeout)
					});
					if (that.options.event == "mouseover") {
						dnf.addEventSimple(that.trigger[index], "mouseout",
						function() {
							that.ct(that.t)
						})
					}
				})(i, this)
			}
		},
		setOptions: function(options) {
			this.options = {
				timeout: 60,
				currCls: "cur",
				disCls: "dis",
				event: "mouseover",
				onFinish: function() {},
				animation: null
			};
			dnf.Extend(this.options, options || {})
		},
		switchTo: function(n) {
			if (this.index == n) {
				return
			} else {
				this.trigger[this.index].className = this.trigger[this.index].className.replace(this.options.currCls, "");
				this.panel[this.index].className = this.panel[this.index].className.replace(this.options.disCls, "")
			};
			this.trigger[n].className += " " + this.options.currCls;
			if (this.options.animation) {
				this.options.animation(n, this.slider)
			} else {
				this.panel[n].className += " " + this.options.disCls
			}
			if (this.onFinish) {
				this.onFinish(n, this.panel[n], this._id)
			}
			this.index = n
		},
		ct: function() {
			clearTimeout(this.t)
		}
	};
	dnf.fillTmpl=function(d){
		var str = '<div class="tab-panel"><ul>',o = dnf.$$('shop-list'),d_length=d.length;
		for (var i = 0; i < d_length; i++) {
			d[i].iSale = Math.ceil((d[i].iPrice / d[i].iOrgPrice) * 10);
			str +='<li><p class="shop-img"><a href="http://daoju.qq.com/dnf/item/' + d[i].iSeqId + '.shtml?ADTAG=gwtj.rec.cf" target="_blank"><img width="50" height="50" src="' + d[i].sGoodsPic + '"></a><div class="shop-info"><p class="shop-text">'+d[i].sGoodsName+'</p><p class="shop-money">'+d[i].iPrice+'点券</p><a href="#" class="shop-btn" iseqid="'+d[i].iSeqId+'">立即购买</a></div></li>'
			if (i != (d_length - 1) && (i + 1) % 3 == 0) {
                str += '</ul></div><div class="tab-panel"><ul>';
            }
        }
        str += "</ul></div>";
		o.innerHTML = str;
		new dnf.slide("shop-tab","shop-hd-i","li","tab-panel",{showF:true});
		$("#shop-list .shop-btn").click(function() {
            var _id = $(this).attr("iseqid");
            var _cur_goods = D.recact.recMap[_id];
            var _other_recs = $.extend({},
            D.recact.recMap);
            var _page = $("#shop-list").children(".tab-panel").index($(this).parents(".tab-panel")) + 1;
            var _place = $(this).parents("ul").children("li").index($(this).parents("li")) + 1;
            delete _other_recs[_id];
            //启动order组件@上报参数制定
            need(["daoju.hx.order"],
            function(order) {
                //如果不进行购买上报可不调用init方法，一般情况是需要上报的
                order.init({
                    biz: D.recact.option.biz,
                    //业务cf
                    scene_id: D.recact.option.scene_id,
                    //上报类型
                    place: _place,
                    //每一组的第几个为
                    page: _page,
                    //第几组
                    uin: D.recact.reportOption.uin,
                    //用户qq
                    recommend_id: D.recact.reportOption.recommend_id
                    //推荐id
                });
                order.buy(_cur_goods);
            });
            return false;
        });
	};
	dnf.slide=function(id, cls1,tag,cls2, options){
		this.trigger = dnf.getElementsByClassName(id,cls1,tag);
		this.count = this.trigger.length;
		this._id = id;
		this._cls1=cls1;
		this._cls2=cls2;
		this.panel = dnf.getElementsByClassName(id, cls2);
		this.slider = dnf.getElementsByClassName(id, "bd")[0];
		this.t = null;
		this.index = 0;
		this.setOptions(options);
		this.onStart = this.options.onStart;
		this.event = this.options.event;
		this.timeout = this.options.timeout;
		this.onFinish = this.options.onFinish;
		this.showF=this.options.showF;
		this.hide=this.options.hide;
		this.init()
	};
	dnf.slide.prototype={
		init: function() {
		//console.log(this._id+this.panel.length)
		if(this.showF){
			this.trigger[0].className += " " + this.options.currCls;
			this.panel[0].className += " " + this.options.disCls;
		}
			for (var i = 0; i < this.count; i++) { (function(index, that) {
					dnf.addEventSimple(that.trigger[index], that.options.event,
					function() {
						that.t = setTimeout(function() {
							that.switchTo(index)
						},
						that.timeout)
					});
					if (that.options.event == "mouseenter" && that.options.hide) {
						dnf.addEventSimple(dnf.$$(that._id), "mouseleave",
						function() {
							that.ct(that.t);
							that.mo(index);
						})
					}
				})(i, this)
			}
		},
		setOptions: function(options) {
			this.options = {
				timeout: 60,
				currCls: "cur",
				disCls: "dis",
				event: "mouseenter",
				onFinish: function() {},
				animation: null
			};
			dnf.Extend(this.options, options || {})
		},
		switchTo: function(n) {
			if (this.showF && this.index == n) {
				return
			} else {
				this.trigger[this.index].className = this.trigger[this.index].className.replace(this.options.currCls, "");
				this.panel[this.index].className = this.panel[this.index].className.replace(this.options.disCls, "")
			};
			this.trigger[n].className += " " + this.options.currCls;
			if (this.options.animation) {
				this.options.animation(n, this.slider)
			} else {
				this.panel[n].className += " " + this.options.disCls
			}
			if (this.onFinish) {
				this.onFinish(n, this.panel[n], this._id)
			}
			this.index = n
		},
		ct: function() {
				clearTimeout(this.t)
		},
		mo:function(o){
			this.trigger[this.index].className=this._cls1;
			this.panel[this.index].className=this._cls2;
			this.index =o=0
		}
	};
	dnf.corp = function() {
		var corp = dnf.$$("teamwork-select"),
		corp_h = corp.getElementsByTagName("h3")[0],
		corp_list = corp.getElementsByTagName("ul")[0];
		corp_h.onmouseover = corp_list.onmouseover = function() {
			corp_list.style.display = "block";
		};
		corp_h.onmouseout = corp_list.onmouseout = function() {
			corp_list.style.display = "none";
		};
	};
