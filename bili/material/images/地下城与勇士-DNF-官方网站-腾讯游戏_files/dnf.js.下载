			}else if(n=='online'){
				html+='<li class="online"><a href="'+chapterData[n][i].surl+'" target="_blank"><h4>'+chapterData[n][i].title+'<s class="sprite-icon icon-chapter-down"></s></h4><div class="sprite-icon icon-chapter"></div><div class="chapter-name">'+chapterData[n][i].subTitle+'</div><div class="chapter-time">'+chapterData[n][i].time+'</div></a></li>';
			}else if(n=='over'){
				html+='<li class="over"><a href="'+chapterData[n][i].surl+'" target="_blank"><h4>'+chapterData[n][i].title+'<s class="sprite-icon icon-chapter-down"></s></h4><div class="sprite-icon icon-chapter"></div><div class="chapter-name">'+chapterData[n][i].subTitle+'</div><div class="chapter-time">'+chapterData[n][i].time+'</div></a></li>';
			}
		}
		dnf.$$("chapter-list").innerHTML+=html
	};
