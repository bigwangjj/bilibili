	dnf.count=function(){if (typeof(pgvMain) == 'function') pgvMain()};
	dnf.getCookie = function(c_name) {
		if (document.cookie.length > 0) {
			var c_start = document.cookie.indexOf(c_name + "=");
			if (c_start != -1) {
				c_start = c_start + c_name.length + 1;
				var c_end = document.cookie.indexOf(";", c_start);
				if (c_end == -1) {
					c_end = document.cookie.length
				}
				return unescape(document.cookie.substring(c_start, c_end))
			}
		}
		return ""
	};
	dnf.setCookie = function(c_name, value, expiredays) {
		var exdate = new Date();
		exdate.setDate(exdate.getDate() + expiredays);
		document.cookie = c_name + "=" + escape(value) + ((expiredays == null) ? "": "; expires=" + exdate.toGMTString())
	};
	dnf.userInfo=function(name){//user_time 头部新闻 user_gl_time 攻略
		if(name=="user_time"){
			if(!dnf.getCookie(name)){
				var myDate=new Date(),userTime=myDate.getTime();
				dnf.setCookie(name,userTime,86400);
				//dnf.guide.init();
			}else{
				var userTime = dnf.getCookie(name);
			};
		}else if(name=="user_gl_time"){
			var myDate=new Date(),userTime=dnf.setTime();
			dnf.setCookie(name,userTime,86400);
		}
		//console.log(name)
		return userTime
	};
	dnf.countPush=function(jsUrl,id){
		dnf.loadScript(jsUrl,function(){
			var a=[],b=0,nowTime=new Date().getTime();
			for(var i=0;i<7;i++){
				if(i==1){return}
				var newsTime=newsIndexData[i].dtReleaseTime_org.replace(/\-/g, "/");
				if(dnf.userInfo("user_time")<new Date(newsTime).getTime()){
					b++;
					if(b>7){
						b=7
					}
				}else{}
				if(b==0){
				dnf.$$(id).style.display="none";
			}else{
					dnf.$$(id).style.display="block";
					dnf.$$(id).className+=" icon-push"+b;
				};
			};

		});
	};
	dnf.setTime=function(){
		var day=new Date().getDate();
		var month=new Date().getMonth()+1;
		if(month<10){
			month="0"+month;
		};
		if(day<10){
			day="0"+day;
		}
		return month+"/"+day;
	}
	dnf.updataRaidersNum=function(jsUrl,id){
		if(dnf.getCookie("user_gl_time")==dnf.setTime()){
			dnf.$$(id).className+=" looked";
		}
		dnf.loadScript(jsUrl,function(){
			var b=0;
			for(var i=0;i<newsIndexData.length;i++){
				var newsTime=newsIndexData[i]["dtReleaseTime"];
				if(newsTime==dnf.setTime()){
					b++;
				}else{}
			}
			if(b==0){
				dnf.$$(id).style.display="none";
			}else{
				dnf.$$(id).style.display="block";
				dnf.$$(id).innerHTML=b;
			};
		});
	};
