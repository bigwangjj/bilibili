			if(contentNum==0){
				dnf.$$("icon-community").style.display="none";
			}if(videoNum==0){
				dnf.$$("icon-video").style.display="none";
			}if(actNum==0){
				dnf.$$("icon-act").style.display="none";
			}
			dnf.$$("icon-community").innerHTML=contentNum;
			dnf.$$("icon-video").innerHTML=videoNum;
			dnf.$$("icon-act").innerHTML=actNum;
		})
