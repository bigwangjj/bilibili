	dnf.gg=function(ids,hgt,pgv){
		var gg = "<div class='adPic'><ul id='expGg'>",
		at = "main.rotate.",
		gs = "",
		len = ids.length - 1,
		pos = oDaTaNew11370,
		src = "http:\/\/ossweb-img.qq.com\/upload\/adw\/",
		sg = dnf.$$(ids[len]),
		nn = 0,
		spd = 5000;
		for (var i = 0; i < len; i++) {
			if(ids[i] == 'idataAdPos1'){

				gg += '<li><a href="#" data-TGAD="11370,11390" onclick="pgvSendClick({hottag:\'' + at + pgv + '' + (i + 1) + '\'});"></a></li>';
			}else if(ids[i] == 'idataAdPos2'){

				gg += '<li><a href="#" data-TGAD="11370,11391" onclick="pgvSendClick({hottag:\'' + at + pgv + '' + (i + 1) + '\'});"></a></li>';
			}else if(ids[i] == 'idataAdPos3'){

				gg += '<li><a href="#" data-TGAD="11370,11392" onclick="pgvSendClick({hottag:\'' + at + pgv + '' + (i + 1) + '\'});"></a></li>';
			}else if(ids[i] == 'idataAdPos4'){

				gg += '<li><a href="#" data-TGAD="11370,11393" onclick="pgvSendClick({hottag:\'' + at + pgv + '' + (i + 1) + '\'});"></a></li>';
			}else if(ids[i] == 'idataAdPos5'){

				gg += '<li><a href="#" data-TGAD="11370,11394" onclick="pgvSendClick({hottag:\'' + at + pgv + '' + (i + 1) + '\'});"></a></li>';
			}else if(ids[i] == 'idataAdPos6'){

				gg += '<li><a href="#" data-TGAD="11370,11395" onclick="pgvSendClick({hottag:\'' + at + pgv + '' + (i + 1) + '\'});"></a></li>';
			}else{
				//gg += '<li><a href="' + pos["pos" + ids[i]][1] + '" target="_blank" onclick="pgvSendClick({hottag:\'' + at + pgv + '' + (i + 1) + '\'});"><img src="' + src + pos["pos" + ids[i]][2] + '" alt="" /></a></li>';
				gg += '<li><a href="#" data-TGAD="11370,11396" onclick="pgvSendClick({hottag:\'' + at + pgv + '' + (i + 1) + '\'});"></a></li>';
			}

			gs += '<li class="ad-btn' + (i + 1) + '"><span></span></li>';
		};
		gg += '</ul></div>';
		gg += '<ul class="ad-icon sprite-icon">' + gs + '</ul>';
		sg.innerHTML = gg;

		var sgul = sg.getElementsByTagName("div")[0],
		num = sg.getElementsByTagName("span"),
		chg = function(ag1, ag2, ag3) {
			sgul.style.marginTop = 0 - ag1 * hgt + "px";
			nn == ag2 ? num[len - 1].className = "": num[ag3].className = "";
			num[ag1].className += " on";
		};
		chg(0, 0, 1);
		var auto = function() {
			if (document.all) {
				sg.style.filter = "revealTrans(duration=1,transition=23)";
				var flt = sg.filters.revealTrans;
				flt.Transition = 23;
				flt.apply();
				flt.play();
			};
			nn > len - 2 ? nn = 0 : nn++;
			chg(nn, 0, nn - 1);
		};
		var stt = function() {
			sgul.move = setInterval(auto, spd);
		},
		end = function() {
			clearInterval(sgul.move);
		};
		for (var i = 0; i < len; i++) {
			num[i].cnt = i;
			num[i].onmouseover = function() {
				end();
				chg(this.cnt, -1, nn);
				nn = this.cnt;
				//pgvSendClick({
					//hottag: at + pgv + ".icon" + (nn + 1)
				//});
			};
			num[i].onmouseout = stt;
		};
		sgul.onmouseover = end;
		sgul.onmouseout = stt;
		stt();
	};
	dnf.DateDiff=function(endTime) {
		var startTime=new Date().toGMTString();
		startTime = startTime.replace(/\-/g, "/");
		endTime = endTime.replace(/\-/g, "/");
		var sTime = new Date(startTime),eTime = new Date(endTime),iDays = 0;
		if(sTime>eTime){
			iDays="已结束";
		}else{
			var hour=parseInt((eTime.getTime() - sTime.getTime()) / parseInt(1000 * 3600))
			if(parseInt((eTime.getTime() - sTime.getTime()) / parseInt(1000 * 3600 * 24))==0){
				iDays="<b>"+hour+"</b>"+"小时后结束";
			}else{
				iDays="<b>"+parseInt((eTime.getTime() - sTime.getTime()) / parseInt(1000 * 3600 * 24))+"</b>"+"天后结束";
			}
		}
	   return iDays;
	};
