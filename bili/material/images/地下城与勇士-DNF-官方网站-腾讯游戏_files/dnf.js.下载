	dnf.actList=["","","","","",""];

	dnf.intActList=function(n){
		dnf.loadScript("http://dnf.qq.com/commActCale/act_cale_info.js", function(){
			var list=[0,0,0,0,0,0];
			if(dnf.actList[n]==""){
				for(var i=1;i<CommActCaleData.list.length;i++){//console.log(i);
					if(list[0]==4&&list[1]==4&&list[2]==4&&list[3]==4&&list[4]==4){
						break;
					}

					if(list[0]<4&&CommActCaleData.list[i][23]=="新手"){
						dnf.actList[0]+='<li'+(list[0]==3?' class="act-o"':'')+'><a href="'+decodeURIComponent(CommActCaleData.list[i][17])+'" target="_blank" onclick="pgvSendClick({hottag:\'main.single.adggs2_'+list[0]+'\'});"><img src="'+decodeURIComponent(CommActCaleData.list[i][18])+'" alt=""></a><p class="gg-time">'+dnf.DateDiff(new Date(parseInt(CommActCaleData.list[i][2])*1000).toGMTString())+'</p></li>';
						list[0]++
					}
					if(list[1]<4&&CommActCaleData.list[i][23]=="活跃"){
						dnf.actList[1]+='<li'+(list[1]==3?' class="act-o"':'')+'><a href="'+decodeURIComponent(CommActCaleData.list[i][17])+'" target="_blank" onclick="pgvSendClick({hottag:\'main.single.adggs3_'+list[1]+'\'});"><img src="'+decodeURIComponent(CommActCaleData.list[i][18])+'" alt=""></a><p class="gg-time">'+dnf.DateDiff(new Date(parseInt(CommActCaleData.list[i][2])*1000).toGMTString())+'</p></li>';
						list[1]++
					}
					if(list[2]<4&&CommActCaleData.list[i][23]=="付费"){
						dnf.actList[2]+='<li'+(list[2]==3?' class="act-o"':'')+'><a href="'+decodeURIComponent(CommActCaleData.list[i][17])+'" target="_blank" onclick="pgvSendClick({hottag:\'main.single.adggs4_'+list[2]+'\'});"><img src="'+decodeURIComponent(CommActCaleData.list[i][18])+'" alt=""></a><p class="gg-time">'+dnf.DateDiff(new Date(parseInt(CommActCaleData.list[i][2])*1000).toGMTString())+'</p></li>';
						list[2]++
					}
					if(list[3]<4&&CommActCaleData.list[i][14]=="6"){
						dnf.actList[3]+='<li'+(list[3]==3?' class="act-o"':'')+'><a href="'+decodeURIComponent(CommActCaleData.list[i][17])+'" target="_blank" onclick="pgvSendClick({hottag:\'main.single.adggs5_'+list[3]+'\'});"><img src="'+decodeURIComponent(CommActCaleData.list[i][18])+'" alt=""></a><p class="gg-time">'+dnf.DateDiff(new Date(parseInt(CommActCaleData.list[i][2])*1000).toGMTString())+'</p></li>';
						list[3]++
					}
					if(list[4]<4&&CommActCaleData.list[i][14]=="7"){
						dnf.actList[4]+='<li'+(list[4]==3?' class="act-o"':'')+'><a href="'+decodeURIComponent(CommActCaleData.list[i][17])+'" target="_blank" onclick="pgvSendClick({hottag:\'main.single.adggs6_'+list[4]+'\'});"><img src="'+decodeURIComponent(CommActCaleData.list[i][18])+'" alt=""></a><p class="gg-time">'+dnf.DateDiff(new Date(parseInt(CommActCaleData.list[i][2])*1000).toGMTString())+'</p></li>';
						list[4]++
					}
					if(list[5]<4&&CommActCaleData.list[i][14]=="1"&&dnf.DateDiff(new Date(parseInt(CommActCaleData.list[i][2])*1000).toGMTString())!=="已结束"){
						dnf.actList[5]+='<li'+(list[5]==3?' class="act-o"':'')+'><a href="'+decodeURIComponent(CommActCaleData.list[i][17])+'" target="_blank" onclick="pgvSendClick({hottag:\'main.single.adggs1_'+list[5]+'\'});"><img src="'+decodeURIComponent(CommActCaleData.list[i][18])+'" alt=""></a><p class="gg-time">'+dnf.DateDiff(new Date(parseInt(CommActCaleData.list[i][2])*1000).toGMTString())+'</p></li>';
						list[5]++
					}

				}
			}
			dnf.$$("act_list").innerHTML=dnf.actList[n];
			//console.log(dnf.actList[5])
		})
	}
