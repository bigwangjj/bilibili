	dnf.tgaVideo=function(){
		dnf.loadScript("http://dnf.qq.com/act/dnfliveinfo/liveinfo.js",function(){//http://dnf.qq.com/act/dnfliveinfo/liveinfo.js
			var liveData=DNF_OFFICIAL_LIVEINFO.data,liveItem=liveData.items,liveItemLength=liveItem.length,liveItemCon="";
			if (typeof(liveItemLength) != 'undefined' && liveItemLength != null) {
				if(liveItemLength>1){liveItemLength=2}
				for(var i=0;i<liveItemLength;i++){
					if(i=="1"){
						liveItemCon+="<li class='video-bd-o'><a href='"+liveItem[i].channel.url+"?ADTAG=media.dnfgw' target='_blank' title='"+unescape(liveItem[i].channel.status)+"'><img src='"+liveItem[i].preview+"' alt='' /><s class='sprite-icon icon-video'></s></a><p>"+unescape(liveItem[i].channel.status)+"</p></li>";
					}else{
						liveItemCon+="<li><a href='"+liveItem[i].channel.url+"?ADTAG=media.dnfgw' target='_blank' title='"+unescape(liveItem[i].channel.status)+"'><img src='"+liveItem[i].preview+"' alt='' /><s class='sprite-icon icon-video'></s></a><p>"+unescape(liveItem[i].channel.status)+"</p></li>";
					}
				}
				dnf.$$('star_list').innerHTML=liveItemCon;
			}else{dnf.$$('star_list').innerHTML="<li>很抱歉，服务器暂时繁忙，请您稍后再试！</li>";}
		})
	};
	dnf.getPropertyCount=function(o){var jsonList=eval(week_act_data), n, count = 0;for(n in jsonList[o]){if(jsonList[o].hasOwnProperty(n)){count++;}}return count;};
	dnf.returnWeekAct=function(n){
		var jsonList=eval(week_act_data),newslist="<ul>";
		if(dnf.getPropertyCount(n)=="0"){
			newslist="没有内容"
		}else{
			for(var x in jsonList[n]){newslist+="<li><s class='sprite-icon week-act-icon'></s><span>"+jsonList[n][x]['time']+"</span><a href="+jsonList[n][x]['surl']+" target='_blank'>"+jsonList[n][x]['title']+"</a></li>";}
		};
		newslist+="</ul>"
		return newslist
	};
	dnf.selectWeek=function(){
		var nowDate=new Date(),nowDateDay=nowDate.getDay();
		switch(nowDateDay){
			case 1:
			dnf.initWeekAct(1);
			break
			case 2:
			dnf.initWeekAct(2);
			break
			case 3:
			dnf.initWeekAct(3);
			break
			case 4:
			dnf.initWeekAct(4);
			break
			case 5:
			dnf.initWeekAct(5);
			break
			case 6:
			dnf.initWeekAct(6);
			break
			case 0:
			dnf.initWeekAct(7);
			break
		}
	};
	dnf.initWeekAct=function(n){
		var index=n-1,link=dnf.$$("week-act-list").getElementsByTagName("ul")[0].getElementsByTagName("li"),act_con=dnf.$$("act-con");
		link[n-1].className="cur";
		act_con.innerHTML=dnf.returnWeekAct(n);
		for(var i=0;i<link.length;i++){
			link[i].onclick=(function(num){
				return function(){
					link[index].className="";
					index=num;
					link[num].className="cur";
					act_con.innerHTML=dnf.returnWeekAct(num+1);
				}
			})(i);
		}
	};
	dnf.initChapter=function(n){
