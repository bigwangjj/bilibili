dnf._hottag={11:"shop",12:"strategy",13:"media",14:"version",15:"brand",16:"race",17:"gamer",18:"newer"},dnf._original="14;13;17;12;15;16;11";//[11]商城,[12]职业攻略,[13]媒体资讯,[14]版本更新,[15]品牌专区,[16]赛事专区,[17]玩家社区,[18]新手引导
dnf._pgvSendPageEditor=function(_data,_type){//console.log(_data)
	var _status=_data.msg.split(",");
	if(_status[0]==1){
		//if(typeof(_data)!='undefined'){//console.log(typeof(_data)!='undefined')
			if(_data.ret==0&&_status[1]==1&&_data.data!=dnf._original){
				var valueList=_data.data.split(";");
				for(i=0;i<valueList.length;i++){
					//console.log('dnfweb.PageValue.Top'+(i+1)+'.'+valueList[i])
					pgvSendClick({hottag:'PageValue.'+_type+'.Top'+(i+1)+'.'+dnf._hottag[valueList[i]]});
				}
			}
			else{
				pgvSendClick({hottag:'PageValue.original'});
			}
		/*}
		else{
			pgvSendClick({hottag:'PageValue.undefined'});
		}*/
	}
	else{
		pgvSendClick({hottag:'PageValue.unlogin'});
	}
};
