	dnf.LinkInfo=function(str,iVideoId){
    	if(str == ''){
    		return 'http://dnf.qq.com/v/detail.shtml?tid='+iVideoId;
    	}else{
    		return 'http://dnf.qq.com/v/go.shtml?tid='+iVideoId;
    	}
    };
	dnf.hotVideo=function(TypeId){
    	for(var x in typeHotObj.msg){
    		if(x == TypeId){
    			var RetHTML = '';
    			var data = typeHotObj['msg'][x];
    			if(typeHotObj['msg'][x]){
    				for(var y=0;y<2;y++){
	    				if(y==1){
	    					RetHTML+='<li class="video-bd-o"><a href="'+dnf.LinkInfo(data[y]['sUrl'],data[y]['iVideoId'])+'" onclick="pgvSendClick({hottag:\'dnfweb.typehot.iType'+x+'.a'+data[y]['iVideoId']+'\'});" target="_blank"><img src="'+data[y]['sIMG']+'" alt="" /><s class="sprite-icon icon-video"></s></a><p>'+data[y]['sTitle']+'</p></li>';
	    				}else{
	    					RetHTML+='<li><a href="'+dnf.LinkInfo(data[y]['sUrl'],data[y]['iVideoId'])+'" onclick="pgvSendClick({hottag:\'dnfweb.typehot.iType'+x+'.a'+data[y]['iVideoId']+'\'});" target="_blank"><img src="'+data[y]['sIMG']+'" alt="" /><s class="sprite-icon icon-video"></s></a><p>'+data[y]['sTitle']+'</p></li>';
	    				}
	    			}
    			}
    			dnf.$$('TypeHot').innerHTML=RetHTML;
    		}
    	}
	};
	dnf.showWeiXin=function(n){
		/*
		if(n=='1'){
			dnf.$$('weixin-tip').style.display="block"
		}else{dnf.$$('weixin-tip2').style.display="block"}
		*/
		if(!n) return
		var n = parseInt(n);
		switch(n){
			case 1:
				dnf.$$('weixin-tip').style.display="block";
				break;
			case 2:
				dnf.$$('weixin-tip2').style.display="block";
				break;
			case 3:
				dnf.$$('weixin-tip3').style.display="block";
				break;
			case 4:
				dnf.$$('weixin-tip4').style.display="block";
				break;
		}

	};
	dnf.hideWeiXin=function(a){
		a.parentNode.style.display='none';
	};
	dnf.guide={
		init:function(){
			var guideHtml='<div class="guide-mask"></div><div class="guide-inner" id="guide-inner"><div class="guide guide-1"><a href="javascript:dnf.guide.next(1);" class="guide-on"></a><a href="javascript:dnf.guide.end();" class="guide-end"></a></div><div class="guide guide-2"><a href="javascript:dnf.guide.next(2);;" class="guide-on"></a><a href="javascript:dnf.guide.end();" class="guide-end"></a></div><div class="guide guide-3"><a href="javascript:dnf.guide.next(3);;" class="guide-on"></a><a href="javascript:dnf.guide.end();" class="guide-end"></a></div><div class="guide guide-4"><a href="javascript:dnf.guide.end();" class="guide-end"></a></div></div>';
			dnf.$$('guide-box').innerHTML=guideHtml;
			this.next(0);
		},
		next:function(n){
			var guide_pannel=dnf.$$('guide-inner').getElementsByTagName('div'),scrollLong=['wrap','adggs1','cartoonGg','chapter']
			for(var i=0;i<guide_pannel.length;i++){
				guide_pannel[i].style.display="none";
			}
			dnf.$$('guide-box').style.display="block";
			dnf.scroller.init(scrollLong[n],800);
			guide_pannel[n].style.display="block"
		},
		end:function(){
			dnf.$$('guide-box').style.display="none";
			dnf.scroller.init("wrap",800);
		}
	};
