			}
			return html*/
			var html="";
			for(var i=0;i<6;i++){
				html+="<li><span>"+newsIndexData[i].dtReleaseTime+"</span><a href="+newsIndexData[i].infoPath+" target='_blank'>"+decodeURI(newsIndexData[i].sTitle)+"</a></li>";
			}
			return html;
		}
	};
