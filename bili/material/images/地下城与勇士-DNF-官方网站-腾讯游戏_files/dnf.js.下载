(function(){
	if(window.dnf){
		return;
	};
	var dnf={};
	dnf.actAdsHtml = null; //保存热门推荐活动的html元素内容
	dnf.ua=navigator.userAgent;
	dnf.$$=function(elem){return document.getElementById(elem);};
	dnf.getElementsByClassName=function(id, className,tagName){
		var o = "string" == typeof id ? dnf.$$(id) : id;
		var all = tagName ? o.getElementsByTagName(tagName) : o.getElementsByTagName("div");
		var elements = new Array();
		for (var i = 0; i < all.length; i++) {
			if (all[i].className != className) continue;
			elements[elements.length] = all[i]
		}
		return elements
	};
	dnf.addEventSimple = function(el, type, handler, capture) {
		if (typeof el.addEventListener != 'undefined') {
			if (type === 'mouseenter') el.addEventListener('mouseover', dnf.withinElement(handler), capture);
			else if (type === 'mouseleave') el.addEventListener('mouseout', dnf.withinElement(handler), capture);
			else el.addEventListener(type, handler, capture)
		} else if (typeof el.attachEvent != 'undefined') el.attachEvent('on' + type, handler);
		else el['on' + type] = handler
	}
	dnf.withinElement=function(handler){return function(e){var parent=e.relatedTarget;while(parent&&parent!=this){try{parent=parent.parentNode}catch(e){break}}if(parent!=this)handler.call(this,e)}};
	dnf.crtElem=function(o){return document.createElement(o);};
	dnf.Extend=function(destination, source){
		for(var property in source){destination[property]=source[property]}return destination
	}
	dnf.initFlash=function(elem,url,w,h){

		var notApple = function() {
			var rs = false;
			if (dnf.ua.indexOf("MSIE") != -1) {
				try {
					if (new ActiveXObject('ShockwaveFlash.ShockwaveFlash')) {
						rs = true
					}
				} catch(e) {}
			} else {
				var np = navigator.plugins;
				if (np && np.length) {
					for (var i = np.length; i--;) {
						if (np[i].name.toLowerCase().indexOf("shockwave") == -1) {
							rs = true
						}
					}
				}
			}
			return dnf.ua.indexOf("iPhone") == -1 && dnf.ua.indexOf("iPad") == -1 && rs
		} ();
		if (notApple) {

			dnf.loadScript("http://ossweb-img.qq.com/images/js/basic/tgswfobj_s.js",
			function() {
				insertSwfV2(elem, url, w, h, {},
				{},
				"10.0")
			})

			/*dnf.loadScript("http://ossweb-img.qq.com/images/js/basic/tgswfobj_s.js",
			function() {
				//var params = { scale:"noscale", wmode:"transparent",align: "middle",allowFullscreen: "true",allowScriptAccess: "always"}
				//swfobject.embedSWF(url, elem, w, h, "9.0.0",null, {}, params);
				insertSwfV2("flashBox","http://cf.qq.com/act/a20160415index/btn-v1.swf?d=0220","290",'418',{},{},"10.0");
			})*/
		} else {}
	};
	dnf.injectSwf=function(arg) {
		var cfg = {
			'wrapId': 'videoCont'
			,'vid': ''
			,'w': 500
			,'h': 300
			,'auto': 0
		};
		for (var i in arg) {
			arg.hasOwnProperty(i) && arg[i] && (cfg[i] = arg[i]) ;
		}

		var swf = '\
			<object id="tenvideo_flash_player_1403119131105" classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000" width="' + cfg.w + '" height="' + cfg.h + '">\
				<param name="Movie" value="http://static.video.qq.com/TencentPlayer.swf?vid='+ cfg.vid +'&autoplay=' + cfg.auto + '&skin=http://imgcache.qq.com/minivideo_v1/vd/res/skins/TencentPlayerMiniSkin.swf">\
				<param name="WMode" value="transparent">\
				<param name="Quality" value="High">\
				<param name="Scale" value="NoScale">\
				<param name="AllowFullScreen" value="true">\
				<param name="AllowScriptAccess" value="always">\
				<embed type="application/x-shockwave-flash" src="http://static.video.qq.com/TencentPlayer.swf?vid='+ cfg.vid +'&autoplay=' + cfg.auto + '&skin=http://imgcache.qq.com/minivideo_v1/vd/res/skins/TencentPlayerMiniSkin.swf" width="'+ cfg.w + '" height="'+ cfg.h +'" quality="high" wmode="transparent" allowScriptAccess="always" allowFullScreen="true">\
			</object>\
			';
		document.getElementById(cfg.wrapId).innerHTML = swf ;
	};
	dnf.loadScript=function(url,callback){
		var script = document.createElement('script');
		var tagSplit=url.split("/");
		var tag=tagSplit[tagSplit.length-3]+tagSplit[tagSplit.length-2]+tagSplit[tagSplit.length-1];
		if(eval('dnf.s'+tag.replace(/\=/g,'').replace(/\./g,'').replace(/\?/g,'').replace(/\_/g,'').replace(/\-/g,'').replace(/\&/g,''))){
			if (callback) {
				callback()
			}
		}
		else{
			if (script.readyState) {
				script.onreadystatechange = function() {
					if (script.readyState == "loaded" || script.readyState == "complete") {
						script.onreadystatechange = null;
						if (callback) {
							callback()
						}
						var tagSplit=url.split("/");
						var tag=tagSplit[tagSplit.length-3]+tagSplit[tagSplit.length-2]+tagSplit[tagSplit.length-1];
						eval('dnf.s'+tag.replace(/\=/g,'').replace(/\./g,'').replace(/\?/g,'').replace(/\_/g,'').replace(/\-/g,'').replace(/\&/g,'')+'=true');
					}
				}
			} else {
				script.onload = function() {
					if (callback) {
						callback()
					}
					var tagSplit=url.split("/");
					var tag=tagSplit[tagSplit.length-3]+tagSplit[tagSplit.length-2]+tagSplit[tagSplit.length-1];
					eval('dnf.s'+tag.replace(/\=/g,'').replace(/\./g,'').replace(/\?/g,'').replace(/\_/g,'').replace(/\-/g,'').replace(/\&/g,'')+'=true');
				}
			}
			script.type = "text/javascript";
			script.src = url;
			document.body.appendChild(script);

		}

	};


	dnf.creation=function(id,className,_html,_delayLoad){
		var _box=document.createElement("div");
		_box.id=id;
		//if(_delayLoad){_box.setAttribute('id','haha');}
		_box.className=className;
		_box.setAttribute('delayload',"dnf.exposure('"+id+"')");
		_box.innerHTML=_html;
		document.getElementById("main").appendChild(_box);

	};
	dnf.exposure=function(id){
		pgvSendClick({hottag:'main.exposure.'+id});
		if(id=="raiders"){
			dnf.showUpdataNum();
		}
	}
	dnf.setshop=function(){
		dnf.creation("shop","box",'<div class="modle-tit"><div class="modle-tit-more"><a href="http://dnfcity.qq.com" target="_blank">更多 <s class="sprite-icon icon-more"></s></a></div><s class="sprite-icon icon-tit"></s><s class="sprite-icon icon-tit-shop"></s></div><ul class="shop_list q" id="shop_list"></ul>');
		var shopHTML="",mallIndexDatad;
		dnf.loadScript('http://dnfcity.qq.com/html/mallIndexData.js',function(){
			for(i=0;i<mallIndexData.length;i++){
				shopHTML+='<li><a href="'+mallIndexData[i].wareUrl+'" target="_blank"><img src="'+mallIndexData[i].imagePath+'" width="125" height="131" border="0" alt="" /></a><strong>'+decodeURI(mallIndexData[i].wareName)+'</strong><span class="yj">原价:'+mallIndexData[i].marketPrice+'元</span><span>现价:<em>'+mallIndexData[i].basePrice+'</em>元</span></li>'
			}
			dnf.$$("shop_list").innerHTML=shopHTML;
		});
	};
